 left:25rpx;
 top:25rpx;
 text-align: left;
}


.activity {
  display: flex;
  flex-direction: column;
  border-radius:10px;
  background-color:#Fff;
  align-items: stretch;
  padding: 0 20rpx 40rpx;
  margin:0 20rpx 20rpx 20rpx;
  box-shadow: 0 0 20px rgba(109, 108, 108, 0.729);
}

.horizontal {
  text-align: left;
  margin-left:20rpx;
  
  
}

.storehotevents{
  margin-left:150rpx;
  position:absolute;
  top:-1px;

}

.horizontal .tag {
  display: inline-block;
  background: white;
  color: #6a98df;
  font-size: 34rpx;
  padding: 6rpx 10rpx;
  border-radius: 6rpx;
  margin-right: 10rpx;
}

.horizontal .recommend {
  border-radius: 10rpx;
  border: 3rpx solid #cfd9df;
  background-image: linear-gradient(to right, #fa709a 0%, #fee140 100%);
  color: #fff;
  font-size: 23rpx;
  margin-left: 10rpx;
  margin-right: 20rpx;
  padding: 3rpx 10rpx;
  line-height: 1rem;
}


.horizontal .title {
  font-size: 34rpx;
  font-weight:500;
  color: #333;
  text-align: left;
}

.activity .photo {
  width: 100%; /* 670rpx */
  height: 320rpx;
  box-shadow: 0 10rpx 15rpx rgba(0, 0, 0, 0.1);
  border-radius: 12rpx;
  overflow: hidden;
  margin: 10rpx 0;
}

.intro {
  display: -webkit-box;
  -webkit-line-clamp: 2;
  overflow: hidden;
  -webkit-box-orient: vertical;
  text-overflow: ellipsis;
  font-size:26rpx;
  line-height: 23px;
}

.activity .time {
  font-size: 28rpx;
  color: #999;
  text-align: justify;
  padding-top: 10rpx;
}

.activity .location {
  font-size: 28rpx;
  color: #999;
  text-align: justify;
}

.bottom-tip {
  display: block;
  padding: 40rpx 0;
  font-size: 28rpx;
  color: #999;
  text-align: center;
}

