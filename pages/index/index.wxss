/**index.wxss**/
@import '/font.wxss';

page, input, textarea {
  font-family: "Microsoft YaHei UI", "PingFang SC", -apple-system-font;
}

page {
  margin: 0;
  padding: 0;
  background: #ffffff;
  overflow-x: hidden;
}

.indexpage {
  position: relative;
  bottom: 0;  
  background-color: #fff;
}



.search {
  background: #fff;
  display: flex;
  align-items: center;
}

.search .avatar {
  padding: 20rpx 20rpx 0 20rpx;
}

.search .avatar_r {
  padding: 20rpx 20rpx 0 20rpx;
  float: right;
}

.search .avatar image {
  width: 50rpx;
  height: 50rpx;
  border-radius: 50%;
  margin-right: 40rpx;
}

.search .avatar_r image {
  width: 50rpx;
  height: 50rpx;
  border-radius: 50%;
  margin-right: 40rpx;
  margin-left: 630rpx;
}

.item {
  display: flex;
  align-items: center;
  justify-content: center;
  background: white;
  padding: 60rpx;
  border-bottom: 20rpx solid #dbdbdb;
}

.item image {
  height: 150rpx;
  width: 150rpx;
  border-radius: 50%;
  margin-right: 40rpx;
}

