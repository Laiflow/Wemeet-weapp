/*搜索*/

.search-wrapper {
  position: absolute;
  top: 0;
  left: 40px;
  padding: 20rpx 0;
  width: 80%;
  z-index: 1;
  background-color: #fff;
}

.search-button {
  margin: 0 auto;
  color: rgba(0,0,0,0.5);
  height: 30px;
  width: 100%;
  border-radius: 5px;
  background-color: #F6F6F6;
  font-size: 15px;
  line-height: 30px;
  text-align: center;
  vertical-align: top;
}

.search-wrapper-blue {
  background-color: #1296db;
}

